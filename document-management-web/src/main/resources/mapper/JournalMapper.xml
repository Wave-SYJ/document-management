<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.edu.seu.cose.docmanage.mapper.PaperMapper">

    <insert id="insertJournal">
        insert
        dm_journal(id,title,organizer_office,image,language,issn) value (
        #{id},
        #{title},
        #{organizerOffice},
        #{image},
        #{language},
        #{issn}
        )
    </insert>

    <update id="updateJournal">
        update dm_journal
        set title = #{title},
            organizer_office = #{organizerOffice},
            image= #{image},
            language = #{language},
            issn = #{issn}

        where id = #{id}
    </update>
</mapper>